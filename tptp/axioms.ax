    ! [V_x: 'R',V_lo: 'EndPoint',V_hi: 'EndPoint'] :
      ( ( ( 'elem/2' @ V_x @ ( 'range/2' @ V_lo @ V_hi ) )
       => ( ( 'test-range-hi/2' @ V_x @ V_hi )
          & ( 'test-range-lo/2' @ V_lo @ V_x ) ) )
      & ( ( ( 'test-range-hi/2' @ V_x @ V_hi )

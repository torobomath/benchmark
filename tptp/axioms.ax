       => ( 'elem/2' @ V_x @ ( 'range/2' @ V_lo @ V_hi ) ) ) ) )).

thf('.def_range_equality_axiom',axiom,(
    ! [V_lo1: 'EndPoint',V_hi1: 'EndPoint',V_lo2: 'EndPoint',V_hi2: 'EndPoint'] :
      ( ( ( ( 'range/2' @ V_lo1 @ V_hi1 )
          = ( 'range/2' @ V_lo2 @ V_hi2 ) )
       => ( ( V_lo1 = V_lo2 )
          & ( V_hi1 = V_hi2 ) ) )
      & ( ( ( V_lo1 = V_lo2 )
          & ( V_hi1 = V_hi2 ) )
       => ( ( 'range/2' @ V_lo1 @ V_hi1 )
          = ( 'range/2' @ V_lo2 @ V_hi2 ) ) ) ) )).

